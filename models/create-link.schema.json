{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "create-link",
  "description": "Create link request model",
  "type": "object",
  "required": [ "origin" ],
  "additionalProperties": false,
  "properties": {
    "origin": {
      "type": "string",
      "description": "The URL to be shortened"
    },
    "backhalf": {
      "type": "string",
      "description": "The portion of the URL generated that follows the domain name"
    }
  }
}
